package chapter6;

public class NumCount {
	public static void main(String[] args) {
		int[] nums = new int[10];
		for (int i = 0; i < nums.length; i++){
		nums[i] = ((int)(Math.random() * 10000)) % 1001;
		}
		System.out.println("数组元素");
		for (int i = 0; i < nums.length; i++){
			System.out.print(nums[i] + ",");
		}
		System.out.println();
		int count1 = 0,	count2 = 0;
		for (int i = 0; i < nums.length; i++){
			if(nums[i] %  2 ==0){
				count2++;
			}else{
				count1++;
		}
		
		}
	}

}
